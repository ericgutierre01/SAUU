@model SAUU.Data.Entities.Rol

@{
    ViewData["Title"] = "Detalle";
}

<div class="page-header">
    <h1>
        <i class="fa fa-book page-header-icon"></i>
        <a asp-action="Index">Roles</a> > @ViewData["Title"]
    </h1>
</div>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.nombreRol)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.nombreRol)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.idRol" class="btn btn-labeled btn-primary">Editar</a> |
    <a asp-action="Index" class="btn btn-labeled btn-primary">Ir atras</a>

</div>
